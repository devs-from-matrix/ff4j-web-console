    "@angular/material": "^8.1.3",
    "@angular/platform-browser": "^8.2.2",
    "@angular/platform-browser-dynamic": "^8.2.2",
    "@angular/router": "^8.2.2",
    "ag-grid-angular": "^21.2.2",
    "ag-grid-community": "^21.2.2",
    "bootstrap": "^4.2.1",
    "core-js": "^3.4.0",
    "ngx-logger": "^4.0.7",
    "rxjs": "^6.5.3",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.18",
